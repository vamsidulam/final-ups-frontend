# Frontend Environment Variables
# Copy this file to .env.local and update with your actual values

# Backend API Base URL
# For development: http://localhost:10000/api
# For production: https://your-backend-domain.com/api
VITE_API_BASE_URL=http://localhost:10000/api

# WebSocket URL for real-time updates
# For development: ws://localhost:10000/ws/ups-updates
# For production: wss://your-backend-domain.com/ws/ups-updates
VITE_WS_URL=ws://localhost:10000/ws/ups-updates

# Application Configuration
VITE_APP_NAME=UPS Monitoring System
VITE_APP_VERSION=1.0.0